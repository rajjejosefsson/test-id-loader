{
  "name": "test-id-loader",
  "description": "Remove your test id with Webpack",
  "version": "0.0.0-semantically-released",
  "license": "MIT",
  "author": "Rasmus Josefsson <rajjejosefsson@gmail.com> (@rajjejoseefsson)",
  "contributors": [
    {
      "name": "Rasmus Josefsson",
      "email": "rajjejosefsson@gmail.com"
    }
  ],
  "repository": "https://github.com/rajjejosefsson/test-id-loader.git",
  "bugs": "https://github.com/rajjejosefsson/test-id-loader/issues",
  "homepage": "https://github.com/rajjejosefsson/test-id-loader",
  "main": "lib/index.js",
  "engines": {
    "node": ">= 4"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "commit": "git-cz",
    "validate": "kcd-scripts validate",
    "pretest": "rm -rf jest coverage jsdoc-api dmd test/builds",
    "test": "jest --verbose --coverage",
    "docs": "jsdoc2md lib/index.js > LOADER.md",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "commitizen": "^3.0.7",
    "cz-conventional-changelog": "^2.1.0",
    "del": "^4.0.0",
    "jest": "^24.5.0",
    "jsdoc-to-markdown": "^4.0.1",
    "semantic-release": "^15.13.3",
    "kcd-scripts": "^1.1.1"

  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "keywords": [
    "JS",
    "JavaScript",
    "Loader",
    "TestId",
    "Webpack"
  ]
}
